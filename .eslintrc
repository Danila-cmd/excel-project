{
	"parser":"babel-eslint",
	"env":{
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules":{
		"semi":"off",
		"arrow-parens":"off",
		"comma-dangle":"off",
		"no-tabs":"off",
		"indent":"off",
		"no-mixed-spaces-and-tabs":"off",
		"quotes":"off",
		"space-before-blocks":"off",
		"no-trailing-spaces":"off",
		"padded-blocks":"off",
		"max-len":"off",
		"eol-last":"off",
		"require-jsdoc":"off"
	},
	"extends":["eslint:recommended", "google"]
}